{
    "builds": [
        {
            "src": "wsgi.py",
            "use": "@vercel/python",
            "config": {
        "pythonVersion": "3.11",
        "buildCommand": "pip install -r requirements.txt"
      }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "wsgi.py"
        }
    ]
}